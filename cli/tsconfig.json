{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "declaration": true,
    "sourceMap": true,
    "inlineSourceMap": false,
    "inlineSources": true,
    "outDir": "../dist",
    "rootDir": ".",
    "baseUrl": ".",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "../node_modules",
    "../dist",
    "../frontend"
  ]
}
