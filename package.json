{
  "name": "aicodereview-cli",
  "private": false,
  "version": "1.1.3",
  "description": "AI-powered code review tool with web interface",
  "keywords": [
    "code-review",
    "ai",
    "gemini",
    "claude",
    "static-analysis",
    "cli"
  ],
  "author": "HainanZhao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HainanZhao/AICodeReview.git"
  },
  "homepage": "https://github.com/HainanZhao/AICodeReview#readme",
  "bugs": {
    "url": "https://github.com/HainanZhao/AICodeReview/issues"
  },
  "bin": {
    "aicodereview": "./bin/cli.js"
  },
  "files": [
    "bin/",
    "dist/",
    "backend/dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "workspaces": [
    "backend",
    "shared"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "vite",
    "start:backend": "cd backend && npm run start",
    "build": "npm run build:src && npm run build:frontend && npm run build:backend",
    "build:src": "tsc -p src/tsconfig.json",
    "build:frontend": "vite build",
    "build:backend": "cd backend && npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx,js,jsx",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "prepublishOnly": "npm run build",
    "pack:test": "npm pack && npm install -g ./aicodereview-cli-1.0.0.tgz"
  },
  "dependencies": {
    "@aireview/shared": "workspace:*",
    "@anthropic-ai/sdk": "^0.17.1",
    "@google/generative-ai": "^0.24.1",
    "commander": "^12.1.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.31.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.6.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
